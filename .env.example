# BPJS Kredensial Configuration
# Copy file ini ke .env dan isi dengan kredensial yang sebenarnya

# BPJS General Configuration
BPJS_CONS_ID=consid
BPJS_SECRET_KEY=secretkey

# VCLAIM Configuration
BPJS_VCLAIM_USER_KEY=key_vclaim
BPJS_VCLAIM_BASE_URL=https://apijkn.bpjs-kesehatan.go.id/vclaim-rest/
BPJS_VCLAIM_CDN_URL=https://apijkn-cdn.bpjs-kesehatan.go.id/vclaim-rest/

# ANTROL/ANTREAN Configuration
BPJS_ANTROL_USER_KEY=key_antrol
BPJS_ANTROL_BASE_URL=https://apijkn.bpjs-kesehatan.go.id/antreanrs/
BPJS_ANTROL_CDN_URL=https://apijkn-cdn.bpjs-kesehatan.go.id/antreanrs/

# Monitoring Settings
MONITORING_DEFAULT_INTERVAL=5000
MONITORING_MAX_HISTORY=100
MONITORING_TIMEOUT=30
MONITORING_RETRY_ATTEMPTS=3
